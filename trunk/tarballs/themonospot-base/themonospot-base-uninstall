	rm -rf "$(DESTDIR)$(prefix)/share/themonospot"
	rm -rf "$(DESTDIR)$(programfilesdir)/plugins"
	cd "$(DESTDIR)$(prefix)"
	if test `ls "$(DESTDIR)$(programfilesdir)"| wc -w` -gt 0 ; then \
	    echo "nothing to do"; \
	else \
	    rm -rf "$(DESTDIR)$(programfilesdir)"; \
	    echo "rm -rf '$(DESTDIR)$(programfilesdir)'"; \
	fi 
install-programfilesDATA

	  echo " $(programfilesDATA_INSTALL) '$(THEMONOSPOT_PNG)' '/usr/share/pixmaps/themonospot.png'"; \
	  $(programfilesDATA_INSTALL) "$(THEMONOSPOT_PNG)" "/usr/share/pixmaps/themonospot.png"; \
	  echo " $(programfilesDATA_INSTALL) '$(THEMONOSPOT_DESKTOP)' '/usr/share/applications/themonospot.desktop'"; \
	  $(programfilesDATA_INSTALL) "$(THEMONOSPOT_DESKTOP)" "/usr/share/applications/themonospot.desktop"; \

uninstall-programfilesDATA

	  echo " rm -f '/usr/share/applications/themonospot.desktop'"; \
	  rm -f "/usr/share/applications/themonospot.desktop"; \
	  echo " rm -f '/usr/share/pixmaps/themonospot.png'"; \
	  rm -f "/usr/share/pixmaps/themonospot.png"; \

